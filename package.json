{
  "name": "project-template-generator",
  "version": "1.0.0",
  "authors": [
    "Charles NGUYEN"
  ],
  "license": "MIT",
  "description": "Generate an Angular 1 project with Webpack 2",
  "main": "src/app/index.module.js",
  "scripts": {
    "clean": "npm run clean:build && npm run clean:coverage",
    "clean:build": "rimraf ./build",
    "clean:coverage": "rimraf ./coverage",
    "build": "webpack --config config/webpack.config.dev.js --progress --color --display-error-details --display-cached",
    "prebuild": "npm run clean:build",
    "build:dist": "webpack --config config/webpack.config.prod.js --progress --color --display-error-details --display-cached",
    "prebuild:dist": "npm run clean:build",
    "start": "npm run serve",
    "serve": "webpack-dashboard -- webpack-dev-server --config config/webpack.config.dev.js --env.dashboard=true --progress --colors --open",
    "serve:dist": "webpack-dashboard -- webpack-dev-server --config config/webpack.config.prod.js --env.dashboard=true --progress --colors --open --compress",
    "test": "karma start",
    "test:ci": "karma start karma.conf.ci.js",
    "test:auto": "karma start --auto",
    "lint:es": "eslint --config .eslintrc --fix src",
    "lint:sass": "sass-lint -c .sass-lint.yml 'src/app/**/*.scss' -v -q",
    "stats": "webpack --config config/webpack.config.dev.js --profile -j > stats.json"
  },
  "dependencies": {
    "angular": "1.5.9",
    "angular-animate": "1.5.9",
    "angular-sanitize": "1.5.9",
    "angular-ui-router": "^0.2.15",
    "lodash": "^3.9.3"
  },
  "devDependencies": {
    "angular-mocks": "1.5.9",
    "autoprefixer": "^6.6.1",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-object-rest-spread": "^6.20.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.1.11",
    "babel-preset-react": "^6.16.0",
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "3.6.1",
    "eslint-config-airbnb": "11.1.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-angular": "^1.4.1",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.3.0",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.26.0",
    "invariant": "^2.2.2",
    "isparta-loader": "^2.0.0",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.5.2",
    "karma": "^1.4.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^2.0.2",
    "keycode": "^2.1.8",
    "ng-annotate-loader": "^0.2.0",
    "ngtemplate-loader": "^1.3.1",
    "node-sass": "^4.5.0",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^1.2.2",
    "resolve-url-loader": "^1.6.1",
    "rimraf": "^2.5.4",
    "sass-lint": "^1.8.2",
    "sass-loader": "^5.0.1",
    "sasslint-webpack-plugin": "^1.0.3",
    "stripcomment-loader": "^0.1.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.3.0"
  },
  "engines": {
    "node": ">=6.9.5"
  }
}
